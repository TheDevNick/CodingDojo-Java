package main.java.classes.game;

import main.java.classes.characters.Character;

public class GridSquare {
	public int x;
	public int y;
	public Character character;
	public boolean isWall;
	public Item item;
	public Shop shop;
	
	public GridSquare(int gridRow, int gridColumn, Character gridCharacter, boolean isWall, Item gridItem) {
		this.y = gridRow;
		this.x = gridColumn;
	}
}
