package main.java.classes.characters;

import main.java.classes.tools.Die;

public class Monster extends Character {

		public Monster(String name, String description) {
			super(name, Die.rollDie(5) + 22, 27, 0, rollDie(3) + 7, description);
			
			this.dexterity = 20 - (5 - (this.maxStrength - this.strength));
		}
}
