package main.java.classes.characters;

import main.java.classes.tools.Die;

public class Fighter extends Player {

	public Fighter(String description) {
		super("fighter", Die.rollDie(5) + 17, 22, 0, rollDie(2), description);
		
		this.dexterity = 22 - (5 - (this.maxStrength - this.strength));
	}

}