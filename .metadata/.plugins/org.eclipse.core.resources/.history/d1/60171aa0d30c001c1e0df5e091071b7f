package main.java.classes.game;

public class GridSquare {
	public int row;
	public int column;
	public string character;
	public boolean isWall;
	public string item;
	
	public GridSquare() {
		this.row = row;
		this.column = column;
		this.character = character;
		this.isWall = isWall;
		this.item = item;
	}
}
